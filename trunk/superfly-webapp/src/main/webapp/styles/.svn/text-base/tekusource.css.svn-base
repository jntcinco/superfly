@charset "utf-8";
body {
	margin:0px;
	padding:0px;
}
.ts-pcon {
	width: 100%;
	height: 100%;
	background-color: #707787;
	background-image: url(../images/bg.gif);
	background-repeat: repeat-x repeat-y; 
}
.ts-container {
	width: 780px;
	height: 100%;
	background-color: #FFFFFF;
	margin: 0 auto;
}
img {
	border: none;
}
.tah10 {
	font-family: Tahoma;
	font-size: 10px;
	text-decoration: none;
	color: #000000;
}
.tah11 {
	font-family: Tahoma;
	font-size: 11px;
	text-decoration: none;
	color: #000000;
}
.ver10 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10px;
	text-decoration: none;
	color: #000000;
}
.ver11 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 11px;
	text-decoration: none;
	color: #000000;
}
.tah9 {
	font-family: Tahoma;
	font-size: 9px;
	text-decoration: none;
	color: #000000;
}
.ver9 {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 9px;
	text-decoration: none;
	color: #000000;
}